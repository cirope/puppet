# begin _phpserver.conf
  client_max_body_size 4G;
  server_name _;

  server_tokens off;

  keepalive_timeout 5;

  upload_resumable on;
  upload_max_file_size 0;

  # path for static files
  root /var/www;
  index index.html index.htm index.php;

  fastcgi_intercept_errors on;
  # end _phpserver.conf
